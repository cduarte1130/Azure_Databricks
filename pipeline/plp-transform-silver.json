{
	"name": "plp-transform-silver",
	"properties": {
		"activities": [
			{
				"name": "nb_transform_data",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"notebookPath": "/ConsumosEngerticos/transform_data",
					"baseParameters": {
						"rawPath": "@concat('abfss://raw@adlsdatabrickscmd01.dfs.core.windows.net/ingest/', pipeline().parameters.fileName)",
						"silverPath": "@concat('abfss://silver@adlsdatabrickscmd01.dfs.core.windows.net/', replace(pipeline().parameters.fileName, '.csv','_transformed.parquet'))"
					}
				},
				"linkedServiceName": {
					"referenceName": "LS_Databricks",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"fileName": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Pip_Ingesta_ConsumoEnegertico"
		},
		"annotations": []
	}
}