{
	"name": "DS_Raw_Ingest",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fileName": {
				"type": "string",
				"defaultValue": "consumos_energeticos.csv"
			}
		},
		"folder": {
			"name": "ConsumoEnergetico"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().fileName",
					"type": "Expression"
				},
				"folderPath": "ingest",
				"fileSystem": "raw"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "id_cliente",
				"type": "String"
			},
			{
				"name": "fecha_consumo",
				"type": "String"
			},
			{
				"name": "consumo_kwh",
				"type": "String"
			},
			{
				"name": "tipo_servicio",
				"type": "String"
			},
			{
				"name": "region",
				"type": "String"
			},
			{
				"name": "estado_medidor",
				"type": "String"
			}
		]
	}
}